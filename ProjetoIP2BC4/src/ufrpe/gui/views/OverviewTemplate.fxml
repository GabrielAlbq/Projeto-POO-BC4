<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ufrpe.gui.model.ControladorOverview">
   <children>
      <BorderPane prefHeight="576.0" prefWidth="1024.0">
         <top>
            <MenuBar prefHeight="29.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="Estoque">
                  <items>
                    <MenuItem fx:id="mi_adicionarprod" mnemonicParsing="false" onAction="#AdicionarProduto" text="Adicionar produto" />
                        <MenuItem fx:id="mi_alterarprod" mnemonicParsing="false" text="Alterar produto" />
                        <MenuItem fx:id="mi_removerprod" mnemonicParsing="false" text="Remover produto" />
                        <MenuItem fx:id="mi_buscarprod" mnemonicParsing="false" text="Buscar produto" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Vendas">
                  <items>
                    <MenuItem fx:id="mi_pdv" mnemonicParsing="false" text="Ponto de Venda" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Funcionarios">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
